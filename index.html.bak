<script>
    document.getElementById('contactForm').addEventListener('submit', function (event) {
        event.preventDefault(); // منع إعادة تحميل الصفحة

        // استبدل هذا بالرابط الخاص بـ Microsoft Forms
        var formUrl = "FORM_LINK_HERE";

        // جمع البيانات
        var name = document.getElementById('name').value;
        var email = document.getElementById('email').value;
        var message = document.getElementById('message').value;

        // تحويل البيانات إلى رابط قابل للإرسال إلى Microsoft Forms
        var formData = new URLSearchParams({
            "entry.XXXXXXX": name,  // استبدل XXXXXXX بالمعرّف الخاص بالسؤال في Microsoft Forms
            "entry.YYYYYYY": email, // استبدل YYYYYYY بالمعرّف الخاص بالسؤال في Microsoft Forms
            "entry.ZZZZZZZ": message // استبدل ZZZZZZZ بالمعرّف الخاص بالسؤال في Microsoft Forms
        });

        // إرسال البيانات إلى Microsoft Forms
        fetch(formUrl + "?" + formData.toString(), {
            method: "POST",
            mode: "no-cors"
        }).then(() => {
            document.getElementById('contactForm').reset(); // تفريغ النموذج بعد الإرسال
            document.getElementById('successMessage').classList.remove('hidden'); // إظهار رسالة النجاح
        }).catch(error => console.error("Error:", error));
    });
</script>



